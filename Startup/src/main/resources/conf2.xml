<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">





	<bean id="memberListPrinter" class="com.kosb.chap1.MemberListPrinter">
		<constructor-arg ref="memberDao" />
		<constructor-arg ref="memberPrinter" />
	</bean>

	<bean id="memberRegSvc" class="com.kosb.chap1.MemberRegisterService">
		
	</bean>

	<bean id="changePwdSvc" class="com.kosb.chap1.ChangePasswordService">
		<constructor-arg ref="memberDao" />
	</bean>

	<bean id="infoPrinter" class="com.kosb.chap1.MemberInfoPrinter">
	</bean>

	<bean id="versionPrinter" class="com.kosb.chap1.VersionPrinter">
		<constructor-arg value="4" />
		<constructor-arg value="1" />
	</bean>

</beans>
